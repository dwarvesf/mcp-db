{
  "name": "@dwarvesf/mcp-db",
  "version": "1.0.0",
  "description": "MCP server for database operations",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "mcp-db": "bin/mcp-db"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "NODE_OPTIONS=\"--loader ts-node/esm\" node --experimental-specifier-resolution=node src/cli.ts",
    "debug": "mcp-inspector -- NODE_OPTIONS=\"--loader ts-node/esm\" node --experimental-specifier-resolution=node src/cli.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwarvesf/mcp-db.git"
  },
  "keywords": [
    "mcp",
    "database",
    "postgres",
    "duckdb",
    "gcs"
  ],
  "author": "Dwarves Foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dwarvesf/mcp-db/issues"
  },
  "homepage": "https://github.com/dwarvesf/mcp-db#readme",
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@modelcontextprotocol/sdk": "^1.8.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "arg": "^5.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "duckdb": "^0.10.0",
    "express": "^5.0.1",
    "pg": "^8.11.3",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "ts-node": "^10.9.2"
  }
}
